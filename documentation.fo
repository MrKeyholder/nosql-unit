	<fo:inline font-weight="bold">private</fo:inline> <fo:inline font-weight="bold">static</fo:inline> <fo:inline font-weight="bold">final</fo:inline> Logger LOGGER = LoggerFactory.getLogger(BookManager.<fo:inline font-weight="bold">class</fo:inline>);

	<fo:inline font-weight="bold">private</fo:inline> <fo:inline font-weight="bold">static</fo:inline> <fo:inline font-weight="bold">final</fo:inline> MongoDbBookConverter MONGO_DB_BOOK_CONVERTER = <fo:inline font-weight="bold">new</fo:inline>	MongoDbBookConverter();
	<fo:inline font-weight="bold">private</fo:inline> <fo:inline font-weight="bold">static</fo:inline> <fo:inline font-weight="bold">final</fo:inline> DbObjectBookConverter DB_OBJECT_BOOK_CONVERTER = <fo:inline font-weight="bold">new</fo:inline> DbObjectBookConverter();

			
	<fo:inline font-weight="bold">private</fo:inline> DBCollection booksCollection;

	<fo:inline font-weight="bold">public</fo:inline> BookManager(DBCollection booksCollection) {
