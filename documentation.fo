	}

	<fo:inline font-weight="bold">public</fo:inline> <fo:inline font-weight="bold">void</fo:inline> setNumberOfPages(<fo:inline font-weight="bold">int</fo:inline> numberOfPages) {
		<fo:inline font-weight="bold">this</fo:inline>.numberOfPages = numberOfPages;
	}


	<fo:inline font-weight="bold">public</fo:inline> String getTitle() {
