<fo:inline font-weight="bold">import</fo:inline> com.lordofthejars.nosqlunit.redis.RedisRule;

<fo:inline font-weight="bold">public</fo:inline> <fo:inline font-weight="bold">class</fo:inline> WhenYouFindABook {

	<fo:inline font-weight="bold">static</fo:inline> {
		System.setProperty(<fo:inline font-weight="bold" font-style="italic">"REDIS_HOME"</fo:inline>, <fo:inline font-weight="bold" font-style="italic">"/opt/redis-2.4.16"</fo:inline>);
	}

	<fo:inline color="gray">@ClassRule</fo:inline>
	<fo:inline font-weight="bold">public</fo:inline> <fo:inline font-weight="bold">static</fo:inline> ManagedRedis managedRedis = newManagedRedisRule().build();

	<fo:inline color="gray">@Rule</fo:inline>
	<fo:inline font-weight="bold">public</fo:inline> RedisRule redisRule = newRedisRule().defaultManagedRedis();
	
	<fo:inline color="gray">@Test</fo:inline>
	<fo:inline color="gray">@UsingDataSet(locations="book.json", loadStrategy=LoadStrategyEnum.CLEAN_INSERT)</fo:inline>
	<fo:inline font-weight="bold">public</fo:inline> <fo:inline font-weight="bold">void</fo:inline> book_should_be_returned_if_title_is_in_database() {
		
		BookManager bookManager = <fo:inline font-weight="bold">new</fo:inline> BookManager(<fo:inline font-weight="bold">new</fo:inline> Jedis(<fo:inline font-weight="bold" font-style="italic">"localhost"</fo:inline>));
		Book findBook = bookManager.findBookByTitle(<fo:inline font-weight="bold" font-style="italic">"The Hobbit"</fo:inline>);
		
		assertThat(findBook, is(<fo:inline font-weight="bold">new</fo:inline> Book(<fo:inline font-weight="bold" font-style="italic">"The Hobbit"</fo:inline>, 293)));
		
	}

}</fo:block></fo:block></fo:block><fo:block space-before.optimum="1em" space-before.minimum="0.8em" space-before.maximum="1.2em">And dataset used is:
